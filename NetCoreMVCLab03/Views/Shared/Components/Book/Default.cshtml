@model IEnumerable<NetCoreMVCLab03.Models.Book>

@foreach (var item in Model)
{
	<div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
		<div class="img-thumbnail">
			<img src="@item.Image" width="100" height="200"/>
			<div class="caption-top">
				<h3>@item.Title</h3>
				<p>
					Price: @item.Price
				</p>
			</div>
		</div>
	</div>
}